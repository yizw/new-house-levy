<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    
    <include layout="@layout/ty_title_bar_refresh"/>   
    
    
    <LinearLayout
            android:id="@+id/house_house"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">  
      <ImageView 
        android:layout_height="wrap_content"
		android:layout_width="30dp"		
		android:layout_marginLeft="10dp"
		android:src="@drawable/sign_blue"/> 
		
  	  <Button android:id="@+id/btn_house"
		android:layout_width="60dp"
		android:layout_height="43dp"
		android:textSize="16sp"
		android:text="房开商"
		android:layout_weight="1"
		android:textColor = "@color/bluegreen"
		android:gravity="left|center_vertical"/>	
		
	<ImageView android:layout_height="wrap_content"
		android:layout_width="30dp"
		android:layout_marginRight="15dp"
		android:src="@drawable/list_arrow"/>		
         </LinearLayout>
    
    
    
    <LinearLayout
            android:id="@+id/house_pro"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">  
      <ImageView 
        android:layout_height="wrap_content"
		android:layout_width="30dp"		
		android:layout_marginLeft="10dp"
		android:src="@drawable/sign_blue"/> 
		
  	  <Button android:id="@+id/btn_pro"
		android:layout_width="60dp"
		android:layout_height="43dp"
		android:textSize="16sp"
		android:text="征收项目"
		android:layout_weight="1"
		android:textColor = "@color/bluegreen"
		android:gravity="left|center_vertical"/>	
		
	<ImageView android:layout_height="wrap_content"
		android:layout_width="30dp"
		android:layout_marginRight="15dp"
		android:src="@drawable/list_arrow"/>		
         </LinearLayout>
    
    

</LinearLayout>
